{
  "comments": false,
  "env": {
    "main": {
      "presets": [
        ["env", {
          "targets": { "node": 7 }
        }]
      ]
    },
    "renderer": {
      "presets": [
        ["env", {
          "modules": false
        }]
      ]
    },
    "web": {
      "presets": [
        ["env", {
          "modules": false
        }]
      ]
    }
  },
  "plugins": [
    "transform-runtime",
    ["component", {
      "libraryName": "element-ui",
      "styleLibraryName": "theme-chalk"
    }]]
}
